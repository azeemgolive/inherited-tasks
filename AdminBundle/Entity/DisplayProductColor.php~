<?php

namespace LoveThatFit\AdminBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * LoveThatFit\AdminBundle\Entity\DisplayProductColor
 *
 * @ORM\Table(name="displayProduct_colors")
 * @ORM\Entity(repositoryClass="LoveThatFit\AdminBundle\Entity\DisplayProductColorRepository")
 */
class DisplayProductColor
{
   
    
    /**
     * @var integer $id
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
    
    /**
     * @var string $colortitle
     *
     * @ORM\Column(name="colortitle", type="string")     
     */
    
}